  errorMessage String?

  @@index([deployedAt])
  @@index([status])
  @@index([environment])
}

model system_settings {
  id        String   @id @default(uuid())
  key       String   @unique
  value     String
  updatedAt DateTime @updatedAt
  updatedBy String?
}

model staff_announcements {
  id             String               @id @default(uuid())
  title          String
  content        String
  createdAt      DateTime             @default(now())
  createdBy      String
  createdByEmail String
  expiresAt      DateTime?
  isActive       Boolean              @default(true)
  updatedAt      DateTime             @updatedAt
  priority       AnnouncementPriority @default(NORMAL)

  @@index([isActive])
  @@index([createdAt])
  @@index([priority])
}

model staff_activity_feed {
  id          String   @id @default(uuid())
  department  String?
  action      String
  description String?
  metadata    Json?
  createdAt   DateTime @default(now())
  entityId    String?
  entityName  String?
  entityType  String?
  userEmail   String
  userId      String?
  userName    String?

  @@index([userId])
  @@index([department])
  @@index([createdAt])
  @@index([action])
}

model department_access_log {
  id         String   @id @default(uuid())
  department String
  action     String
  createdAt  DateTime @default(now())
  ipAddress  String?
  pagePath   String
  userAgent  String?
  userEmail  String
  userId     String

  @@index([userId])
  @@index([department])
  @@index([createdAt])
  @@index([pagePath])
}

